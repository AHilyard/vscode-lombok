{
    "name": "vscode-lombok",
    "displayName": "Lombok Annotations Support for VS Code",
    "description": "A lightweight extension to support Lombok annotations processing in Visual Studio Code",
    "version": "1.0.0",
    "publisher": "GabrielBB",
    "icon": "images/icon.png",
    "license": "MIT",
    "engines": {
        "vscode": "^1.42.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/GabrielBB/vscode-lombok.git"
    },
    "keywords": [
        "java",
        "lombok"
    ],
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "main": "./out/index",
    "activationEvents": [
        "onLanguage:java",
        "workspaceContains:pom.xml",
        "workspaceContains:build.gradle"
    ],
    "contributes": {
        "javaExtensions": [
            "./server/lombok-bundle.jar"
        ]
    },
    "scripts": {
        "clean": "node ./node_modules/rimraf/bin.js out/",
        "compile": "npm run clean && tsc -p ./",
        "test": "npm run compile && node ./out/test/runTest.js",
        "watch": "tsc -watch -p ./",
        "bundle": "jar cvfm ./server/lombok-bundle.jar ./server/MANIFEST.MF ./server/lombok.jar",
        "package": "npm run bundle && vsce package",
        "vscode:prepublish": "npm run compile"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^7.0.43",
        "@types/vscode": "1.21.0",
        "glob": "^7.1.6",
        "mocha": "^6.1.4",
        "rimraf": "^3.0.1",
        "tslint": "^6.0.0",
        "typescript": "^3.7.5",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {}
}
